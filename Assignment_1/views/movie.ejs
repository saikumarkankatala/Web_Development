
<h1 style="text-align: center"> Movies Page</h1>
<head> 
 <style>
    th, td {
  text-align: left;
  padding: 8px;
}
 </style>

<body>

 <table class = "center" style="margin-left: auto;margin-right: auto;overflow-x:auto;border: 1px solid #ddd;">
    <tr>
        <th>
            Movie Name
        </th>
        <th>
            Hero
        </th>
        <th>
            Heroine
        </th>
        <th>
            Director
        </th>
    </tr>
    <% movieNames.forEach(function(i) { 
        %>
    <tr>
     <td>  <%= i.name %><br></td>
     <td><%= i.Hero %><br></td>
     <td><%= i.Heroine %><br></td>
     <td><%= i.director %><br></td>
    
    </tr>
  <% }); %>
 </table>

</body>
</head>
